    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.2",
    "graphql": "^16.6.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3"
