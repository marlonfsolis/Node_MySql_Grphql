{
  "name": "node-mysql-graphql",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dist": "node dist/bin/www.js",
    "start:dev": "set DEBUG=node-mysql-graphql:* & nodemon src/bin/www.ts",
    "build:prod": "tsc --project tsconfig.prod.json",
    "build:dist": "tsc --build",
    "build:dev": "tsc --build --watch",
    "build:cp": "tscp ",
    "build:cpw": "tscp -w",
    "codegen": "graphql-codegen --config src/graphql/codegen.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.4.0",
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/merge": "^8.3.18",
    "@graphql-tools/schema": "^9.0.16",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "config": "^3.3.9",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.3",
    "graphql": "^16.6.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "morgan": "~1.10.0",
    "mysql2": "^3.1.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/introspection": "3.0.0",
    "@graphql-codegen/typescript": "3.0.0",
    "@graphql-codegen/typescript-resolvers": "3.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.0",
    "@types/supertest": "^2.0.12",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "typescript-cp": "^0.1.7"
  }
}
